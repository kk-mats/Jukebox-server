{
	// IntelliSense を使用して利用可能な属性を学べます。
	// 既存の属性の説明をホバーして表示します。
	// 詳細情報は次を確認してください: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Program",
			"runtimeExecutable":"${workspaceRoot}/node_modules/.bin/ts-node-dev",
			"runtimeArgs": [
				"--inspect",
				"--watch",
				"src",
				"-r",
				"dotenv/config",
				"-r",
				"tsconfig-paths/register"
			],
			"args": [
				"${workspaceFolder}/src/application/route/index.ts"
			],
			"sourceMaps": true,
			"cwd": "${workspaceRoot}",
			"restart": true,
			"protocol": "inspector",
			"internalConsoleOptions": "openOnSessionStart",
			"port": 9229
		}
	]
}